    "version": "0.2.0",
    "configurations": 
    [
        {
            "name": "GDB",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/Build/JeepPS.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "device": "STM32f103C8",
            "interface": "swd",
            "configFiles": 
            [
                "interface/stlink.cfg",
                "target/stm32f1x.cfg",
            ],
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            },
            "svdFile": "STM32F103.svd"
        }
    ]
}
